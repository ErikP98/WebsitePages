            "exponential",
            1.2],
          [
            "zoom"
          ],
          6.5, 0, 8, 0.5, 20, 13]
      }
    },
    
    
    {
      "id": "label_country_3",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 2,
      "maxzoom": 9,
      "filter": [
        "all",
        [
          "==",
          [
            "get",
            "class"
          ],
          "country"
        ],
        [
          "\u003E=",
          [
            "get",
            "rank"
          ],
          3]
      ],
      "layout": {
        "text-field": [
          "case",
          [
            "has",
            "name:nonlatin"
          ],
          [
            "concat",
            [
              "get",
              "name:latin"
            ],
            "\n",
            [
              "get",
              "name:nonlatin"
            ]
          ],
          [
            "coalesce",
            [
              "get",
              "name_en"
            ],
            [
              "get",
              "name"
            ]
          ]
        ],
        "text-font": [
          "Noto Sans Bold"
        ],
        "text-max-width": 6.25,
        "text-size": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          3, 9, 7, 17]
      },
      "paint": {
        "text-color": "transparent",
        "text-halo-blur": 1,
        "text-halo-color": "transparent",
        "text-halo-width": 1
      }
    },
